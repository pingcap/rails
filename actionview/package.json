{
  "name": "@rails/ujs",
  "version": "7.1.0-alpha",
  "description": "Ruby on Rails unobtrusive scripting adapter",
  "main": "app/assets/javascripts/rails-ujs.js",
  "module": "app/assets/javascripts/rails-ujs.esm.js",
  "files": [
    "app/assets/javascripts/*.js"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
