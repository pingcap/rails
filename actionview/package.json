    "lint": "eslint app/javascript"
  },
  "repository": {
    "type": "git",
    "url": "rails/rails"
  },
  "contributors": [
    "Stephen St. Martin",
    "Steve Schwartz",
    "Dangyi Liu",
    "All contributors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rails/rails/issues"
  },
  "homepage": "https://rubyonrails.org/",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.23.4",
    "rollup": "^2.53.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
